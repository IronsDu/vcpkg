{
  "name": "wiredtiger",
  "version-semver": "11.3.1",
  "description": "High performance, scalable, production quality embedded database (MongoDB storage engine)",
  "homepage": "https://github.com/wiredtiger/wiredtiger",
  "license": "GPL-2.0",
  "dependencies": [
    "lz4",
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    },
    "zlib",
    "zstd"
  ],
  "features": {
    "snappy": {
      "description": "Enable Snappy compression",
      "dependencies": [
        "snappy"
      ]
    },
    "sodium": {
      "description": "Enable libsodium encryption support",
      "dependencies": [
        "libsodium"
      ]
    }
  }
}
